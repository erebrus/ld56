config_version=5

[application]

config/name="Hop Hop Don't Stop"
config/version="0.9.2"
run/main_scene="res://src/start_screen/start_screen.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

Logger="*res://addons/gs_logger/logger.gd"
Types="*res://src/common/types.gd"
Events="*res://src/common/events.gd"
Globals="*res://src/common/globals.tscn"
HyperLog="*res://addons/hyperlog/hyperlog.tscn"

[debug]

gdscript/warnings/unused_signal=0

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/initial_position_type=2
window/size/window_width_override=1280
window/size/window_height_override=720
window/stretch/mode="canvas_items"

[editor_plugins]

enabled=PackedStringArray("res://addons/Todo_Manager/plugin.cfg", "res://addons/gs_logger/plugin.cfg", "res://addons/hyperlog/plugin.cfg", "res://addons/xsm/plugin.cfg")

